{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es6",
			"es7",
			"es2017",
			"dom"
		],
		"module": "esnext",
		"moduleResolution": "node",
		"jsx": "react",
		"sourceMap": true,
		"strict": true,
		"importHelpers": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	}
}